package assignment3;
import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.ArrayList;
import java.util.Random;


public class parser {
	public static void main(String args[]){
		System.out.println("ok");
	}
	public ArrayList<String> get_tables() throws IOException {
		BufferedReader br = new BufferedReader(new FileReader("tables.txt"));
		ArrayList<String> line = new ArrayList<String>();
		boolean foundNextTable = false;
//		while(foundNextTable != true){
			System.out.println(getTableName(br.readLine(), br));
//		}
		br.close();		
		return line;
	}
	
	
	public String getTableName(String line, BufferedReader br) throws IOException{
		StringBuilder sb = new StringBuilder();
		for(int i = 11; i<line.length();i++){
			if(line.charAt(i)==32){
				if(i== line.length()){
					line = br.readLine();
				}
			} 
			else{
				sb.append(line.charAt(i));
			}
		}
		return sb.toString();
	}
}

	